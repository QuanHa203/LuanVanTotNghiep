@using System.Globalization
@model List<string>

@if (Model.Count == 0)
{
    <h4 class="fg-second">Không có video nào</h4>
    return;
}

@foreach (string url in Model)
{
    try
    {
        string fileName = System.IO.Path.GetFileNameWithoutExtension(url);

        <div class="media-box-child">
            <div>
                <video src="@url" onclick="showOverlayMediaSectionElement(this)"></video>
                <div class="overlay-video">
                    <div class="play-circle-icon"></div>
                </div>
            </div>
            <i class="fg-second">@DateTime.ParseExact(fileName, "yyyy-MM-dd_HH-mm-ss", CultureInfo.InvariantCulture).ToString("f")</i>
        </div>
    }
    catch
    {

    }
}